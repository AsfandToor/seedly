    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "inquirer": "^12.6.3",
    "loadash": "^1.0.0",
    "mcps-logger": "^1.0.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.1",
